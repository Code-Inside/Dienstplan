@inherits LayoutComponentBase

<div class="bg-white">
    <div aria-hidden="true" class="relative">
        <img src="https://tailwindui.com/img/ecommerce-images/product-feature-02-full-width.jpg" alt="" class="h-96 w-full object-cover object-center">
        <div class="absolute inset-0 bg-gradient-to-t from-white"></div>
    </div>

    <div class="relative mx-auto -mt-12 max-w-7xl px-4 pb-16 sm:px-6 sm:pb-24 lg:px-8">
        <div class="mx-auto max-w-2xl text-center lg:max-w-4xl">
            <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Dienstplan als Kalendereinträge</h2>
            <p class="mt-4 text-gray-500">Wandel deinen Dienstplan in Kalendereinträge für Android, iPhone & co. um...</p>
        </div>

        <main>
            @Body
        </main>
    </div>
</div>


<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>
